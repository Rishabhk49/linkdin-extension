{
  "manifest_version": 3,
  "name": "LinkedIn Automation",
  "version": "1.0",
  "permissions": [
    "storage",  
    "tabs",
    "scripting",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://www.linkedin.com/*"
  ]
}
